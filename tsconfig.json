{
  "extends": "./node_modules/jscommons/tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "rootDir": "src",
    "outDir": "dist",
    "lib": [
      "es2015",
      "es2016",
      "dom"
    ],
    "typeRoots": [
      "./@types",
      "./node_modules/@types"
    ],
    "sourceMap": true,
    "declaration": true
  },
  "includes": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist/**/*"
  ]
}
