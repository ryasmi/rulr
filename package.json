    "duplication": "jscpd",
    "semantic-release": "ht2-release-public-travisci-lib"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@ht2-labs/semantic-release": "1.0.31",
    "@types/mocha": "2.2.48",
    "@types/node": "9.6.15",
    "assert": "1.4.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "jscommons": "2.1.4",
    "jscpd": "0.6.17",
    "mocha": "4.1.0",
    "nyc": "11.7.3",
    "tslint": "5.10.0",
    "tslint-consistent-codestyle": "1.13.0",
    "tslint-immutable": "4.5.4",
    "typescript": "2.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
