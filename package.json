{
  "name": "rulr",
  "author": "ryansmith94",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansmith94/rulr.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "acp": "git add -A && npm run commit && git push",
